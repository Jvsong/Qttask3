   <string notr="true">/* 设置整个窗口背景颜色 */
QMainWindow {
    background-color: #f5f5f5; /* 浅灰色背景 */
    border: 1px solid #dcdcdc; /* 边框颜色 */
}

/* 标题标签样式 */
QLabel {
    font: bold 14pt &quot;Microsoft YaHei&quot;; /* 粗体字体 */
    color: #333333; /* 深灰色文字 */
}

/* 登录界面标题样式 */
QLabel#label {
    font: bold 18pt &quot;Microsoft YaHei&quot;;
    color: #2d89ef; /* 蓝色文字 */
    text-align: center;
}

/* 输入框样式 */
QLineEdit {
    font: 12pt &quot;Microsoft YaHei&quot;;
    border: 1px solid #cccccc; /* 边框颜色 */
    border-radius: 4px; /* 圆角 */
    padding: 4px; /* 内边距 */
    background-color: #ffffff; /* 白色背景 */
}

QLineEdit:focus {
    border: 1px solid #2d89ef; /* 焦点时的边框颜色 */
    background-color: #f0f8ff; /* 淡蓝色背景 */
}

/* 按钮样式 */
QPushButton {
    font: bold 8pt &quot;Microsoft YaHei&quot;;
    color: #ffffff; /* 白色文字 */
    background-color: #2d89ef; /* 蓝色背景 */
    border: none;
    border-radius: 4px; /* 圆角 */
    padding: 6px 12px;
}

QPushButton:hover {
    background-color: #1e70bf; /* 鼠标悬停时变深的蓝色 */
}

QPushButton:pressed {
    background-color: #144d8a; /* 按下时更深的蓝色 */
}

/* 工具栏样式 */
QToolBar {
    background-color: #e6e6e6; /* 浅灰色背景 */
    border-bottom: 1px solid #dcdcdc; /* 下边框 */
}

/* 状态栏样式 */
QStatusBar {
    background-color: #f5f5f5;
    border-top: 1px solid #dcdcdc;
    font: 10pt &quot;Microsoft YaHei&quot;;
    color: #666666;
}

/* 工具栏图标样式 */
QToolButton {
    border: none;
    background-color: transparent;
}

QToolButton:hover {
    background-color: #f0f0f0; /* 鼠标悬停时背景变浅 */
}
</string>
